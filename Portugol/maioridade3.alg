algoritmo "semnome"
// Função :
// Autor :
// Data : 11/03/2013
// Seção de Declarações 
var
ano, anoatual, idade: inteiro
inicio
anoatual <- 2013
escreval ("Entre com o seu ano de nascimento")
leia (ano)
idade <- anoatual - ano
se (idade>=18) entao
escreval ("Maior de Idade")
senao
escreval ("Menor de Idade")
fimse

// Seção de Comandos 
fimalgoritmo
